<h1>Add Item</h1>
<form id="addItem-form" autocomplete="off" action="/admin/dashboard/add" method="post">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" required><br><br>
    
    <label for="weight">Weight:</label>
    <input type="text" id="weight" name="weight" required><br><br>
    
    <label for="calories">Calories:</label>
    <input type="number" id="calories" name="calories" required><br><br>
    
    <label for="fat">Fat:</label>
    <input type="number" step="0.01" id="fat" name="fat" required><br><br>
    
    <label for="carbs">Carbs:</label>
    <input type="number" step="0.01" id="carbs" name="carbs" required><br><br>
    
    <label for="protein">Protein:</label>
    <input type="number" step="0.01" id="protein" name="protein" required><br><br>
    
    <label for="image">Image:</label>
    <input type="text" id="image" name="image" required><br><br>
    <div id="error-message" style="display: none; color: red;"></div>
    <input type="submit" value="Add Item">
</form>
